#include list
#include core

proc main in
    var list = make_list();

    var i = 0;
    printf("Append to list\n");
    while i < 10 do
        list_append(list, i);
        i = i + 1;
    done

    printf("Enumerate the list\n");
    var cur = list_head(list);
    while cur do
        printf("%d\n", list_node_data(cur));
        cur = list_node_next(cur);
    done

    free_list(list);
    return 0;
end
