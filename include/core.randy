
#include syscall
#include string
#include file

extern puts cstr;
extern printf fmt, ...;
extern fprintf file, fmt, ...;
extern fflush file;
extern putchar char;
extern abort;

extern stdin;
extern stdout;
extern stderr;

proc exit code in
    fflush(u64@(stdout));
    fflush(u64@(stderr));
    syscall(SYS_exit, code);
end